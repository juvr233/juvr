{
  "name": "soul-chronicle-server",
  "version": "1.0.0",
  "description": "Soul Chronicle Divination API Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "db:init": "ts-node src/scripts/initDb.ts",
    "db:migrate": "ts-node src/scripts/dbMigration.ts",
    "db:backup": "ts-node src/scripts/dbBackup.ts",
    "db:restore": "ts-node src/scripts/dbBackup.ts restore",
    "db:seed": "ts-node src/scripts/initDivinationMetadata.ts",
    "ai:init": "ts-node src/scripts/initAiModels.ts",
    "ai:tarot:init": "ts-node src/scripts/initTarotAiModel.ts"
  },
  "keywords": [
    "divination",
    "tarot",
    "iching",
    "numerology",
    "api"
  ],
  "author": "Soul Chronicle Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^2.0.4",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/csurf": "^1.11.5",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.5",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.15",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
